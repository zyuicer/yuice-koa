const Koa = require("koa");
<% if (middleware.bodyparser) {%>
const bodyParser = require("koa-bodyparser");
<% } %>  

const app = new Koa();

<% if(middleware.bodyparser) {%>
app.use(bodyParser());
<% } %>  

app.listen(<%= port %> , () => {
  console.log("service run http://localhost:<%= port %>");
});
